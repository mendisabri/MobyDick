@{
    ViewBag.Title = "Home Page";
}
@using MobyDickSonuc.Models
@model IEnumerable<Sozcukler>
@{
    Layout = null;
}


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <table border="1" cellpadding="0" cellspacing="0">
        <tr>
            <th>Customer Id</th>
            <th>Name</th>
        </tr>
        @{
            var data = ViewBag.Message;
        }
        @foreach (Sozcukler sonuc in data)
        {
            <tr>
                <td>@sonuc.Sozcuk</td>
                <td>@sonuc.GecisSayisi</td>
            </tr>
        }
    </table>
</body>
</html>